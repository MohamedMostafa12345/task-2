void main() {
  int num1 = 20;
  int num2 = 10;

  int sum = num1 + num2;
  int product = num1 * num2;
  int difference = num1 - num2;

  print("Sum: $sum");
  print("Product: $product");
  print("Difference: $difference");

  if (num1 > num2) {
    print("$num1 is greater than $num2");
  } else if (num1 < num2) {
    print("$num2 is greater than $num1");
  } else if (num1 == num2) {
    print("Both numbers are equal.");
  }
}
