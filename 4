void main() {
 int grade = 70;
  if (grade >= 90 && grade <= 100) {
    print("A");
  } else if (grade >= 80 && grade <= 89) {
    print("B");
  } else if (grade >= 70 && grade <= 79) {
    print("C");
  } else if (grade < 70 && grade >= 0) {
    print("F");
  } else {
    print("Invalid grade!");
  }
}
